{
  "name": "@lskjs/telegraf",
  "version": "3.22.0",
  "description": "LSK.js â€“ telegraf - wrapper for Telegraf/Telegram",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           echo ok",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/algos": "^3.17.0",
    "@lskjs/env": "^3.18.0",
    "@lskjs/log": "^3.17.0",
    "fishbird": "^0.21.0",
    "p-timeout": "^4.1.0",
    "typegram": "^4.3.0"
  },
  "devDependencies": {
    "telegraf": "^4.12.2"
  },
  "peerDependencies": {
    "telegraf": "^4.12.2"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config",
    "rules": {
      "import/named": "warn"
    }
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    "./middlewares": {
      "import": "./lib/middlewares/index.mjs",
      "types": "./lib/middlewares/index.d.ts",
      "default": "./lib/middlewares/index.js"
    },
    "./middlewares/*": {
      "import": "./lib/middlewares/*.mjs",
      "types": "./lib/middlewares/*.d.ts",
      "default": "./lib/middlewares/*.js"
    },
    "./commands": {
      "import": "./lib/commands/index.mjs",
      "types": "./lib/commands/index.d.ts",
      "default": "./lib/commands/index.js"
    },
    "./commands/*": {
      "import": "./lib/commands/*.mjs",
      "types": "./lib/commands/*.d.ts",
      "default": "./lib/commands/*.js"
    },
    "./utils": {
      "import": "./lib/utils/index.mjs",
      "types": "./lib/utils/index.d.ts",
      "default": "./lib/utils/index.js"
    },
    "./utils/*": {
      "import": "./lib/utils/*.mjs",
      "types": "./lib/utils/*.d.ts",
      "default": "./lib/utils/*.js"
    }
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs/telegraf",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs/telegraf",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
