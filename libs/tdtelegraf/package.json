    "axios": "^1.6.0",
    "fishbird": "^0.21.0",
    "prebuilt-tdlib": "^0.1008019.0",
    "tdl": "^7.4.0",
    "tdl-tdlib-addon": "^1.2.2",
    "telegraf": "^4.15.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
